        "moment": "^2.29.2",
        "node-sass": "^4.12.0",
        "node-sass-chokidar": "^1.3.4",
        "npm-run-all": "^4.1.5",
        "react": "^16.8.6",
        "react-custom-scrollbars": "^4.2.1",
        "react-dom": "^16.8.6",
        "react-redux": "^7.0.3",
        "react-router-dom": "^5.0.0",
